{
  "coc.preferences.formatOnSaveFiletypes": ["javascript", "javascript.jsx", "typescript", "typescript.tsx", "typescriptreact", "vue", "dart", "prisma", "terraform", "tf", "graphql"],
  "codeLens.enable": true,
  "suggest.enablePreview": true,
  "suggest.floatConfig": {
    "border": true
  },
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextPrefix": " ⇒ ",
  "diagnostic.virtualTextLineSeparator": " \\ ",
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.errorSign": "✘",
  "diagnostic.warningSign": "⚠",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "",
  "diagnostic.floatConfig": {
    "border": true
  },
  "signature.floatConfig": {
    "border": true
  },
  "hover.floatConfig": {
    "border": true
  },

  "solargraph.commandPath": "/Users/fp/.rbenv/shims/solargraph",

  "git.addGBlameToBufferVar": true,

  "markdownlint.config": {
    "line_length": false
  },

  "languageserver": {
    "terraform": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": ["terraform"],
      "initializationOptions": {},
      "settings": {}
    }
  },

  "typescript.format.enabled": false,
  "javascript.format.enabled": false,
  "typescript.referencesCodeLens.enable": false,
  "typescript.implementationsCodeLens.enable": false,
  "javascript.referencesCodeLens.enable": false,
  "javascript.implementationsCodeLens.enable": false,

  "java.format.settings.url": "/Users/fp/.config/coc/extensions/node_modules/redhat.java/eclipse-formatter.xml",
  "java.errors.incompleteClasspath.severity": "ignore",

  "json.schemas": [{
    "fileMatch": [
      "firebase.json"
    ],
    "url": "https://raw.githubusercontent.com/firebase/firebase-tools/master/schema/firebase-config.json"
  }],

  "flutter.autoOpenDevLog": true,
  "flutter.closingLabelPrefix": "<- ",
  "dart.lineLength": 120
}
