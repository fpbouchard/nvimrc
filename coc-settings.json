{
  "coc.preferences.formatOnSaveFiletypes": ["javascript", "javascript.jsx", "typescript", "typescript.tsx", "vue"],
  "suggest.enablePreview": true,
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextPrefix": " > ",
  "git.addGBlameToBufferVar": true,
  "prettier.printWidth": 120,
  "prettier.eslintIntegration": true,
  "eslint.filetypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue"
  ],
  "solargraph.useBundler": true,
  "vetur.dev.vlsPath": "/usr/local/lib/node_modules/vls",
  "vetur.format.defaultFormatter.html": "prettier",
  "vetur.format.defaultFormatter.js": "prettier-eslint",
  "vetur.trace.server": "verbose",
  "codeLens.enable": true,
  "omnisharp.trace.server": "verbose",
  "languageserver": {
    "dart": {
      "command": "dart_language_server", // in windows is dart_language_server.bat
      "args": [],
      "filetypes": ["dart"],
      "initializationOptions": {},
      "settings": {
        "dart": {
          "validation": {},
          "completion": {}
        }
      }
    },
    "terraform": {
      "command": "terraform-lsp",
      "filetypes": ["terraform"],
      "initializationOptions": {}
    }
  }
}
