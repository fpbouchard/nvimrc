{
  "coc.preferences.formatOnSaveFiletypes": ["javascript", "javascript.jsx", "typescript", "typescript.tsx", "vue", "dart"],
  "suggest.enablePreview": true,
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextPrefix": " > ",
  "codeLens.enable": true,

  "solargraph.useBundler": true,

  "git.addGBlameToBufferVar": true,

  "markdownlint.config": {
    "line_length": false
  },

  "languageserver": {
    "terraform": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": ["terraform", "tf"],
      "initializationOptions": {}
    }
  },

  "typescript.format.enabled": false,
  "javascript.format.enabled": false,
  "typescript.referencesCodeLens.enable": false,

  "volar.formatting.enable": false,
  "prettier.disableLanguages": [],

  "prettier.printWidth": 120,

  "java.format.settings.url": "/Users/fpbouchard/.config/coc/extensions/node_modules/redhat.java/eclipse-formatter.xml",
  "java.errors.incompleteClasspath.severity": "ignore"
}
